{
    "name": "libero/jats-content-workflow",
    "description": "JATS content workflow",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Libero\\JatsContentWorkflow\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "fluentdom/fluentdom": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/promises": "^1.3",
        "guzzlehttp/psr7": "^1.5",
        "league/flysystem": "^1.0",
        "libero/content-api-bundle": "^0.1",
        "libero/media-type": "^0.1",
        "psr/http-message": "^1.0",
        "symfony/http-foundation": "^4.2",
        "symfony/workflow": "^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
