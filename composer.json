{
    "name": "libero/content-store",
    "description": "Implementation of the Libero content API",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Libero\\ContentStore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\Libero\\ContentStore\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "aws/aws-sdk-php-symfony": "^2.0",
        "csa/guzzle-bundle": "^3.1",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.5",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "libero/api-problem-bundle": "^0.2",
        "libero/content-api-bundle": "^0.1",
        "oneup/flysystem-bundle": "^3.1",
        "symfony/config": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/routing": "^4.2",
        "symfony/translation": "^4.2",
        "symfony/workflow": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "csa/guzzle-cache-middleware": "^1.0",
        "league/flysystem-memory": "^1.0",
        "libero/coding-standard": "^0.4",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-symfony": "^0.11",
        "phpunit/phpunit": "^8.0",
        "symfony/debug": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/stopwatch": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/web-profiler-bundle": "^4.2",
        "thomasweinert/phpunit-xpath-assertions": "dev-master",
        "twistor/flysystem-stream-wrapper": "^1.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
